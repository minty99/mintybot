# MintyBot Environment Variables
# Copy this file to .env and fill in your values

# Discord bot token
MINTYBOT_DISCORD_TOKEN=your_discord_token_here

# OpenAI API token
MINTYBOT_OPENAI_TOKEN=your_openai_token_here

# Developer user ID for DM notifications
MINTYBOT_DEV_USER_ID=your_discord_user_id_here
